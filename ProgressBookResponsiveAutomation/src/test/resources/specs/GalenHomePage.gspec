
@objects
  
    main_logo css .header-logo
    user_icon css #currentUserMenu
    central_admin xpath //*[text()='CentralAdmin']
    data_map_logo css .SoftwareAnswers-DataMap-applicationLogo
	grade_book_logo css .SoftwareAnswers-GradeBook-applicationLogo
	special_services_logo css .SoftwareAnswers-SpecialServices-applicationLogo
	special_services_classic_logo css .SoftwareAnswers-SpecialServicesClassic-applicationLogo
	student_information_logo css .SoftwareAnswers-StudentInformation-applicationLogo
	
= Homepage Logo section =
   main_logo:
       width 203px
       height 45px 
       near user_icon 1088px left
       width 14 % of screen/width
         

= Homepage user icon section =
   user_icon:
       width 65px
       height 32px
       near main_logo 1088px  right
       width 4 % of screen/width  
       
= central admin heading section =
  	central_admin:
       width 85px
       height 16px
       width 6 % of screen/width

       
= data map logo section =
   data_map_logo:
       width 333px
       height 60px 
       width 24 % of screen/width
       near grade_book_logo 10px left 
       near special_services_classic_logo 10px top
             
      
= grade book logo section =
   grade_book_logo:
       width 333px
       height 60px 
       width 24 % of screen/width
       near special_services_logo 10px left 
       near data_map_logo 10px right 
       
= special services logo section =
   special_services_logo:
       width 333px
       height 60px 
       width 24 % of screen/width
       near grade_book_logo 10px right        
       
= special services classic logo section =
   special_services_classic_logo:
       width 333px
       height 60px 
       width 24 % of screen/width
       near data_map_logo 10px bottom 
       
= student information logo section =
   student_information_logo:
       width 333px
       height 70px 
       width 24 % of screen/width
       near grade_book_logo 20px bottom 
       
                            